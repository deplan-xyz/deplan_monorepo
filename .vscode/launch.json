{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "deplan_connect",
      "cwd": "${workspaceFolder}/apps/deplan_connect",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--web-port",
        "10001"
      ]
    },
    {
      "name": "deplan_v1",
      "cwd": "${workspaceFolder}/apps/deplan_v1",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "10002"
      ]
    },
    {
      "name": "deplan_v2",
      "cwd": "${workspaceFolder}/apps/deplan_v2",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "10003"
      ]
    },
    {
      "name": "phorevr_v1",
      "cwd": "${workspaceFolder}/apps/phorevr_v1",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "10004"
      ]
    },
    {
      "name": "phorevr_v2",
      "cwd": "${workspaceFolder}/apps/phorevr_v2",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "10005"
      ]
    },
    {
      "name": "subdoor (local)",
      "cwd": "${workspaceFolder}/apps/subdoor",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "10006",
        "--dart-define=ENV=local"
      ]
    },
    {
      "name": "subdoor (dev)",
      "cwd": "${workspaceFolder}/apps/subdoor",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "10006",
        "--dart-define=ENV=dev"
      ]
    },
  ],
}